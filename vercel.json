{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "packages/frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "packages/backend/src/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/backend/src/index.ts"
    },
    {
      "src": "/health",
      "dest": "packages/backend/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}


